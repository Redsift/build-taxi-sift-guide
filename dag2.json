{
  "inputs":{
    "emails": {
      "taxi": {
        "filter": {
          "conditions": [{
            "from": {
              "regexp": {
                "flags": "i",
                "pattern": "billing@hailocab\\.com"
              }
            }
          },{
            "subject": {
              "regexp": {
                "flags": "i",
                "pattern": "^\\s*Addison Lee Booking"
              }
            }
          }],
          "operator": "OR"
        }
      }
    }
  },
  "nodes": [{
      "#": "Messages mapper",
      "implementation": {
          "node": "server/map1.js"
      },
      "input": {
          "bucket": "taxi"
      },
      "outputs": {
          "receipts": {}
      }
    }],
  "stores": {},
  "outputs":{
    "exports":{
      "receipts":{
        "key$schema":"string/string"
      }
    }
  }
}