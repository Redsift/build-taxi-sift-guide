{
  "inputs":{
    "emails": {
      "taxi": {
        "filter": {
          "conditions": [{
            "from": {
              "regexp": {
                "flags": "i",
                "pattern": "billing@hailocab\\.com" 
              }
            }
          }],
          "wants": [
                "archive",
                "htmlBody",
                "textBody"
            ]
        }
      }
    }
  },
  "nodes": [{
    "#": "node1",
    "input": {
      "bucket": "taxi"
    },
    "outputs": {
      "messages": {}
    }
  },{
    "#": "node2",
    "input": {
      "bucket":"messages"
    },
    "outputs": {
      "example1":{}
    }
  }],
  "stores": {
    "messages": {
      "key$schema": "string"
    }
  },
  "outputs":{
    "exports":{
      "example1":{
        "key$schema":"string"
      }
    }
  }
}